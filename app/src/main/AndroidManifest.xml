<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="by.lykashenko.demon.mirparfumanew">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:name=".MirParfumaApp"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StartActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BrendActivity"
            android:label="@string/app_name"
            android:theme="@style/AnimationActivity"
            android:screenOrientation="portrait"
            android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="AA_DB_NAME"
            android:value="Parfum" />
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1" />
        <meta-data
            android:name="AA_MODELS"
            android:value="by.lykashenko.demon.mirparfumanew.Table.Favorites,
            by.lykashenko.demon.mirparfumanew.Table.Trash,
            by.lykashenko.demon.mirparfumanew.Table.BrenduCount,
            by.lykashenko.demon.mirparfumanew.Table.Type,
            by.lykashenko.demon.mirparfumanew.Table.Year,
            by.lykashenko.demon.mirparfumanew.Table.Country,
            by.lykashenko.demon.mirparfumanew.Table.ListParfum,
            by.lykashenko.demon.mirparfumanew.Table.BrenduAll,
            by.lykashenko.demon.mirparfumanew.Table.ForTable,
            by.lykashenko.demon.mirparfumanew.Table.Nota,
            by.lykashenko.demon.mirparfumanew.Table.Semeistvo,
            by.lykashenko.demon.mirparfumanew.Table.Parfums" />
    </application>

</manifest>